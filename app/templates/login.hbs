<div>
    <form {{action "validateParams" crendential on="submit"}}>
        <div class="error">
            {{#if errorMessage}}
              {{errorMessage}}
            {{/if}}
        </div>
        <div>
            <label>
                User Name
            </label>
            {{input value=crendential.username type="text"}}
        </div>
        <div>
            <label>
                Password
            </label>
            {{input value=crendential.password type="password"}}
        </div>
        <button type="submit">login</button>
        <button type="bottunm" {{action 'cancel'}}>cancel</button>
    </form>
</div>
{{outlet}}
